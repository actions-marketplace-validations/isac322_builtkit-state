{
  "name": "builtkit-state",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc && ncc build -s -m -o dist/restore src/main.ts --license license.txt && ncc build -s -m -o dist/save src/post.ts --license license.txt",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/isac322/buildkit-state.git"
  },
  "keywords": [
    "actions",
    "docker",
    "buildkit",
    "cache"
  ],
  "author": {
    "name": "Byeonghoon yoo",
    "email": "bhyoo@bhyoo.com"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/cache": "^3.1.2",
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "dockerode": "^3.3.4"
  },
  "devDependencies": {
    "@types/dockerode": "^3.3.14",
    "@types/node": "^18.11.18",
    "@vercel/ncc": "^0.36.0",
    "prettier": "^2.8.2",
    "typescript": "^4.9.4"
  }
}
